<div class="row">
  <div class="col-md-12">
    <br>
    <h2>Courses</h2>
    <hr />

    <table class="table">
      <thead>
        <tr>
          <th>Course ID</th>
          <th>Course Code</th>
          <th>Course Description</th>
        </tr>
      </thead>
      <tbody>
        {{#each courses}}
          <tr>
            <td>{{this.courseId}}</td>
            <td>{{this.courseCode}}</td>
            <td>{{this.courseDescription}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>

  </div>
</div>
